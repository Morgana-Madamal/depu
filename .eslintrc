{
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "jest/lowercase-name": "error",
    "jest/no-hooks": "error",
    "jest/prefer-to-be-null": "error",
    "jest/prefer-to-be-undefined": "error"
  }
}
